   "source": [
    "def power_xy(x, y):\n",
    "    return x**y\n",
    "\n",
    "power_results = []\n",
    "pairs = [[5, 2], [3, -1], [4, 3], [2, 0]]\n",
    "for x, y in pairs:\n",
    "    if y < 0:\n",
    "        continue\n",
    "    power_results.append(power_xy(x, y))\n",
    "print(power_results)\n"
   ],
